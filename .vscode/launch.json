{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Example Run Configuration",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/tsx",
      "runtimeArgs": ["src/index.ts", "aoc", "2024", "25"],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    }
  ]
}
